{
  "name": "graph-relational-benchmark",
  "version": "1.0.0",
  "description": "Benchmark: Graph vs Relational DB | MSc CS Thesis",
  "scripts": {
    "start": "npm-run-all -p serve server",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "python3 manage.py runserver",
    "setup": "cd backend/api/db && reset_sql.sh -u && reset_cypher.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VictorWinberg/graph-relational-benchmark.git"
  },
  "keywords": [
    "graph",
    "relational",
    "benchmark",
    "database"
  ],
  "author": "Jan Zubac and Victor Winberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VictorWinberg/graph-relational-benchmark/issues"
  },
  "homepage": "https://github.com/VictorWinberg/graph-relational-benchmark#readme",
  "dependencies": {
    "echarts": "^4.2.0-rc.2",
    "element-ui": "^2.5.2",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.5",
    "v-charts": "^1.19.0",
    "vue": "^2.5.21",
    "vue-awesome": "^3.4.0",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
